
# - Config file for the DVC package
# It defines the following variables
#  CCPiDVC_INCLUDE_DIRS - include directories for DVC
#  CCPiDVC_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(CCPIDVC_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CCPIDVC_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET foo AND NOT CCPIDVC_BINARY_DIR)
  include("${CCPIDVC_CMAKE_DIR}/CCPiDVCTargets.cmake")
endif()
 
# These are IMPORTED targets created by CCPiDVCTargets.cmake
set(CCPIDVC_LIBRARIES @CCPI_DVC_BUILD_LIBRARIES@)